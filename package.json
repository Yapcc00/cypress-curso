{
  "name": "cypress-curso",
  "version": "1.0.0",
  "description": "",
  "main": "index.js", "scripts": {
    "limpiar-reporte": "rimraf ./allure-reports && rimraf ./allure-results",
    "ejecutar-pruebas": "npx cypress run --headed --env allure=true",
    "generar-reporte": "allure generate allure-results -o allure-report -c",
    "abrir-reporte": "allure open allure-report",
    "test": "npm run limpiar-reporte && npm run ejecutar-pruebas && npm run generar-reporte && npm run abrir-reporte",
    "cy:parallel" : "npm run limpiar-reporte && cypress-parallel -s ejecutar-pruebas -t 2 -d ./cypress/e2e/tests; npm run generar-reporte"
  },
  "keywords": [],
  "author": "Yeison Cordoba",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.13.0",
    "cypress-parallel": "^0.13.0"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.39.1",
    "allure-commandline": "^2.22.1"
  }
}